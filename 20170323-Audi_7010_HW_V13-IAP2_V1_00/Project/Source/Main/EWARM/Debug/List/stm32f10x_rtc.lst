    271            * @retval None
    272            */
    273          void RTC_ClearFlag(uint16_t RTC_FLAG)
    274          {
    275              /* Check the parameters */
    276              assert_param(IS_RTC_CLEAR_FLAG(RTC_FLAG));
    277          
    278              /* Clear the corresponding RTC flag */
    279              RTC->CRL &= (uint16_t)~RTC_FLAG;
    280          }
